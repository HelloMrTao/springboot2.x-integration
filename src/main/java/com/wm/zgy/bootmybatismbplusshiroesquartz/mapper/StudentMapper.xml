<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wm.zgy.bootmybatismbplusshiroesquartz.mapper.StudentMapper">
    <select id="getAllStudentList" resultType="com.wm.zgy.bootmybatismbplusshiroesquartz.pojo.Student">
        SELECT `stId`,`stName`,`stAge`,`stGender`,`classId`,`hobbyGroupId`,`stHome`,`cityId`
        FROM student
    </select>

    <update id="updateStudent" parameterType="com.wm.zgy.bootmybatismbplusshiroesquartz.pojo.Student">
        update student
        <set>
            <if test="key != null">
                key =#{key},
            </if>
            <if test="student !=null">
                student = #{student}
            </if>
        </set>
        <where>
            key = #{key}
        </where>
    </update>
</mapper>
